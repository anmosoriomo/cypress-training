{
    "compilerOptions": {
        "target": "ES5",
        "skipLibCheck": true,
        "strict": true,
        "lib": [
            "ES5",
            "DOM"
        ],
        "types": [
            "cypress",
            "cypress-file-upload",
            "node"
        ]
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ],
    "exclude": [
        "node_modules/"
    ]
}
